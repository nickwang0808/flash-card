{
  "name": "flash-card",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run --config tests/integration/vitest.config.ts",
    "test:e2e": "playwright test",
    "test": "vitest run",
    "validate:schema": "vitest run tests/unit/schema-validation.test.ts",
    "knip": "knip",
    "check": "tsc -b && knip",
    "seed": "npx tsx scripts/seed-test-repo.ts seed",
    "seed:reset": "npx tsx scripts/seed-test-repo.ts reset",
    "seed:status": "npx tsx scripts/seed-test-repo.ts status",
    "migrate:v2": "npx tsx scripts/migrate-v2.ts"
  },
  "dependencies": {
    "@octokit/rest": "^22.0.1",
    "@supabase/supabase-js": "^2.95.3",
    "@tailwindcss/vite": "^4.1.18",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "rxdb": "^16.21.1",
    "ts-fsrs": "^4.5.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.0",
    "@testing-library/jest-dom": "^6.6.0",
    "@testing-library/react": "^16.1.0",
    "@types/node": "^25.2.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "dotenv": "^17.2.3",
    "jsdom": "^25.0.0",
    "knip": "^5.83.1",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.6.0",
    "vite": "^6.0.0",
    "vite-plugin-pwa": "^0.21.0",
    "vitest": "^2.1.0"
  }
}
